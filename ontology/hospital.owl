<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/hospital#"
     xml:base="http://www.semanticweb.org/hospital"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.semanticweb.org/hospital"/>

    <!-- Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/hospital#Surgeon"/>
    <owl:Class rdf:about="http://www.semanticweb.org/hospital#Operation"/>
    <owl:Class rdf:about="http://www.semanticweb.org/hospital#Equipment"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hospital#canPerform">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hospital#Surgeon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/hospital#Operation"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/hospital#requiresEquipment">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/hospital#Operation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/hospital#Equipment"/>
    </owl:ObjectProperty>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#S001">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Surgeon"/>
        <canPerform rdf:resource="http://www.semanticweb.org/hospital#OP001"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#S002">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Surgeon"/>
        <canPerform rdf:resource="http://www.semanticweb.org/hospital#OP002"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#OP001">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Operation"/>
        <requiresEquipment rdf:resource="http://www.semanticweb.org/hospital#OrthoKit"/>
        <requiresEquipment rdf:resource="http://www.semanticweb.org/hospital#XRayMachine"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#OP002">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Operation"/>
        <requiresEquipment rdf:resource="http://www.semanticweb.org/hospital#LaparoscopyKit"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#OrthoKit">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Equipment"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#XRayMachine">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Equipment"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/hospital#LaparoscopyKit">
        <rdf:type rdf:resource="http://www.semanticweb.org/hospital#Equipment"/>
    </owl:NamedIndividual>

</rdf:RDF>
